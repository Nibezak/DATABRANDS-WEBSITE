---
const { class: className } = Astro.props
---

<svg
  class={className}
  width="40"
  height="40"
  viewBox="0 0 24 24"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <line x1="6" y1="4" x2="6" y2="20" stroke="currentColor" stroke-width="2"></line>
  <line x1="6" y1="4" x2="16" y2="9" stroke="currentColor" stroke-width="2"></line>
  <line x1="6" y1="20" x2="16" y2="15" stroke="currentColor" stroke-width="2"></line>
  <line x1="16" y1="9" x2="16" y2="15" stroke="currentColor" stroke-width="2"></line>
  <circle cx="6" cy="4" r="2" fill="currentColor"></circle>
  <circle cx="6" cy="20" r="2" fill="currentColor"></circle>
  <circle cx="16" cy="9" r="2" fill="currentColor"></circle>
  <circle cx="16" cy="15" r="2" fill="currentColor"></circle>
</svg>
